function [ output_args ] = descriptormaker( img, points, r, c,  )
%DESCRIPTORMAKER Summary of this function goes here
%   Detailed explanation goes here

imgrows = size(img, 1);
imgcols = size(img, 2);
for i = 1:size(r,1)
    if(r(i) >= 5 && c(i) >= 5 &&  r(i) <= imgrows - 4 && c(i) <= imgcols - 4)
        im1desc(k,:) = reshape(img(r(i)-4:r(i)+4, c(i)-4:c(i)+4), [1,81]);
        points1(k,:) = [c1(i), r1(i)];
        k = k + 1;
    end
end

end

